{
  "name": "qrloop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env SKIP_IMAGE_OPTIMIZER=true vite build",
    "build:production": "cross-env SKIP_IMAGE_OPTIMIZER=true vite build --mode production",
    "build:nocheck": "cross-env SKIP_ESLINT=true SKIP_IMAGE_OPTIMIZER=true vite build",
    "netlify": "node scripts/netlify-build.js && npm run fix-image-deps || true && npm run fix-camera-controls || true && npm run generate-pwa-assets || true && npm run build:nocheck",
    "netlify:clean": "cross-env SKIP_IMAGE_OPTIMIZER=true npm run netlify", 
    "prebuild": "npm run lint:build",
    "postbuild": "node scripts/simple-optimize.js || true",
    "optimize": "node scripts/simple-optimize.js || true",
    "analyze": "vite-bundle-analyzer",
    "lint": "eslint ./src --max-warnings=0",
    "lint:fix": "eslint ./src --fix",
    "lint:build": "eslint ./src",
    "preview": "vite preview",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "generate-pwa-assets": "node scripts/generate-pwa-assets.js",
    "fix-warnings": "node scripts/fix-eslint-warnings.js",
    "fix-file": "node scripts/fix-specific-file.js",
    "fix-callback": "node scripts/fix-use-callback.js",
    "fix-image-deps": "node scripts/fix-image-deps.js",
    "fix-camera-controls": "node scripts/fix-camera-controls.js",
    "postinstall": "npm run fix-image-deps || true && npm run fix-camera-controls || true"
  },
  "dependencies": {
    "@react-three/drei": "^10.5.0",
    "@react-three/fiber": "^9.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "jsqr": "^1.4.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
    "qr-code-styling": "^1.9.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.3",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.5.6",
    "react-helmet": "^6.1.0",
    "sharp": "^0.33.3",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.8",
    "vite-bundle-analyzer": "^1.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^0.20.5",
    "workbox-window": "^7.3.0"
  },
  "optionalDependencies": {
    "gifsicle": "^7.0.1",
    "jpegtran-bin": "^7.0.0",
    "optipng-bin": "^9.0.0",
    "pngquant-bin": "^8.0.1"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.8.2"
  }
}
